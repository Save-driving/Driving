<%- model_class = Aseguradora -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:documento_id) %>:</strong></dt>
  <dd><%= @aseguradora.documento_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero_doc) %>:</strong></dt>
  <dd><%= @aseguradora.numero_doc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombres) %>:</strong></dt>
  <dd><%= @aseguradora.nombres %></dd>
  <dt><strong><%= model_class.human_attribute_name(:direccion) %>:</strong></dt>
  <dd><%= @aseguradora.direccion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:municipio_id) %>:</strong></dt>
  <dd><%= @aseguradora.municipio_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefonos) %>:</strong></dt>
  <dd><%= @aseguradora.telefonos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @aseguradora.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:calificacion_id) %>:</strong></dt>
  <dd><%= @aseguradora.calificacion_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observaciones) %>:</strong></dt>
  <dd><%= @aseguradora.observaciones %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              aseguradoras_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_aseguradora_path(@aseguradora), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              aseguradora_path(@aseguradora),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
