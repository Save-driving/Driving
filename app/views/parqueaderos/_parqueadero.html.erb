<tr id=<%= "parqueadero_#{parqueadero.id}" %>>
  <td><%= parqueadero.nombres %></td>
  <td><%= parqueadero.direccion %></td>
  <td><%= parqueadero.municipio.nombre %></td>
  <td><%= parqueadero.telefonos %></td>
  <td><%= parqueadero.email %></td>
  <td><%= parqueadero.observaciones %></td>
  <td>
    <%= link_to image_tag('zoom.png'), parqueadero, :remote => true, :title => "Mostrar" %>
    <%= link_to image_tag('editar.png'), edit_parqueadero_path(parqueadero), :title => "Editar", :remote => true %>
    <%= link_to image_tag('trash.png'), parqueadero, :title => "Eliminar", 
        :data => { :confirm => 'Esta Seguro?' },
        :method => :delete, :remote => true %>
  </td>
</tr>
